{
  "name": "ikoconnect-new",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build:css": "tailwindcss -i src/styles/globals.css -o public/tailwind.css --minify",
    "generate-sitemap": "node scripts/generate-sitemap.js",
    "generate-job-posts": "node scripts/generate-job-posts.js",
    "generate-og-images": "node scripts/generate-og-images.js",
    "check-og-images": "node scripts/check-og-images.js",
    "download-logos": "node scripts/download-logos.js",
    "build": "npm run generate-affiliate-posts && next build && npm run generate-sitemap",
    "generate-affiliate-posts": "node scripts/generate-affiliate-posts.js",
    "find-unused-exports": "ts-prune src --ignore \"src/app/head.tsx\" --ignore \".next\"",
    "find-orphans": "madge --orphans src",
    "start": "next start -p 3000",
    "update-jobs": "node scripts/update-jobs.js"
  },
  "dependencies": {
    "@vercel/og": "^0.6.8",
    "framer-motion": "^12.10.1",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.508.0",
    "marked": "^15.0.11",
    "next": "^15.3.1",
    "next-pwa": "^5.6.0",
    "next-themes": "^0.4.6",
    "node-fetch": "^2.7.0",
    "nodemailer": "^7.0.3",
    "puppeteer": "^24.8.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "remark": "^15.0.1",
    "remark-html": "^16.0.1",
    "satori": "^0.12.2",
    "sharp": "^0.34.1",
    "slugify": "^1.6.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.3.2",
    "@tailwindcss/postcss": "^4.1.5",
    "@tailwindcss/typography": "^0.5.16",
    "@types/debug": "^4.1.12",
    "@types/estree": "^1.0.7",
    "@types/json-schema": "^7.0.15",
    "@types/next-pwa": "^5.6.9",
    "@types/node": "^20.17.47",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.3.1",
    "globby": "^14.1.0",
    "madge": "^8.0.0",
    "postcss": "^8.5.3",
    "postcss-import": "^16.1.0",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "ts-prune": "^0.10.3",
    "typescript": "^5.8.3"
  }
}
